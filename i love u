import turtle
turtle.shape('turtle')
turtle.pencolor('pink')
turtle.speed(1)

turtle.forward(50)
turtle.right(180)
turtle.penup()
turtle.forward(25)
turtle.pendown()
turtle.left(90)
turtle.forward(100)
turtle.right(90)
turtle.penup()
turtle.forward(25)
turtle.pendown()
turtle.left(180)
turtle.forward(50)

turtle.penup()
turtle.forward(50)
turtle.pendown()
turtle.forward(50)
turtle.penup()
turtle.right(180)
turtle.forward(50)
turtle.pendown()
turtle.right(90)
turtle.forward(100)

turtle.right(90)
turtle.penup()
turtle.forward(100)
turtle.pendown()
turtle.right(180)
turtle.circle(50)

turtle.left(180)
turtle.penup()
turtle.forward(60)
turtle.pendown()
turtle.right(60)
turtle.forward(105)
turtle.left(125)
turtle.forward(105)
turtle.right(65)

turtle.penup()
turtle.forward(25)
turtle.pendown()
turtle.forward(50)
turtle.penup()
turtle.right(180)
turtle.forward(50)
turtle.pendown()
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.penup()
turtle.right(180)
turtle.forward(50)
turtle.pendown()
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.penup()
turtle.forward(60)
turtle.pendown()

turtle.left(90)
turtle.forward(100)
turtle.penup()
turtle.left(180)
turtle.forward(100)
turtle.pendown()
turtle.left(90)
turtle.forward(60)
turtle.left(90)
turtle.forward(100)

turtle.mainloop()
